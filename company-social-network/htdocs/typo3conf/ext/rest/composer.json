{
    "name": "cundd/rest",
    "type": "typo3-cms-extension",
    "description": "REST API for TYPO3 CMS",
    "keywords": [
        "typo3cms",
        "rest",
        "api",
        "webservice",
        "service",
        "restful"
    ],
    "require": {
        "php": "~5.6 || ^7.0",
        "ext-ctype": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "psr/http-message": "^1.0.1",
        "psr/log": "^1.0.2",
        "zendframework/zend-diactoros": "^1.3.7",
        "cundd/test-flight": "^0.1.5",
        "phpspec/prophecy": "*",
        "ext-simplexml": "*",
        "ext-curl": "*"
    },
    "homepage": "https://rest.cundd.net/",
    "authors": [
        {
            "name": "Daniel Corn",
            "email": "info@cundd.net"
        }
    ],
    "license": "GPL-2.0-or-later",
    "autoload": {
        "psr-4": {
            "Cundd\\Rest\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cundd\\Rest\\Tests\\": "Tests/"
        }
    },
    "replace": {
        "rest": "self.version",
        "typo3-ter/rest": "self.version"
    }
}
